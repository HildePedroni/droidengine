<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="br.com.ies2.testedroidengine"
	android:versionCode="1"
	android:versionName="1.0" >

	<uses-sdk
		android:minSdkVersion="14"
		android:targetSdkVersion="19" />

	<application
		android:allowBackup="true"
		android:icon="@drawable/ic_launcher"
		android:label="@string/app_name"
		android:theme="@style/AppTheme" >
		<activity
			android:name="br.com.ies2.testedroidengine.activities.MainActivity"
			android:label="@string/app_name" >
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name="br.com.ies2.testedroidengine.activities.SpriteActivity"
			android:label="@string/title_activity_sprite" >
		</activity>
		<activity
            android:name="br.com.ies2.testedroidengine.activities.SvgActivity"
            android:label="@string/title_activity_svg" >
        </activity>
	</application>

</manifest>